CREATE TABLE user_info (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    cookies TEXT
);

getAllUsers:
SELECT * FROM user_info;

insertUser:
INSERT INTO user_info (name) VALUES (?);

updateUserCookiesWithName:
UPDATE user_info
SET cookies = ?
WHERE name = ?;

deleteUserByName:
DELETE FROM user_info
WHERE name = ?;

getUserByName:
SELECT * FROM user_info
WHERE name = ?;


